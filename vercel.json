{
  "version": 2,
  "rewrites": [


    {
      "source": "/admin",
      "destination": "/admin/index.html",
      "source": "/(.*)",
      "destination": "/"
    },


    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}
